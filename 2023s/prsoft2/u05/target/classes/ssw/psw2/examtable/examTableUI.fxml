<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ssw.psw2.examtable.Controller">
	<center>
		<TableView fx:id="tableView" BorderPane.alignment="CENTER">
		</TableView>
	</center>

	<bottom>
		<HBox spacing="10">
			<padding>
				<Insets top="5" bottom="5" right="5" left="5" />
			</padding>

			<Button fx:id="addResultButton" text="Add Result"/>
			<Button fx:id="removeResultButton" text="Remove Result"/>
		</HBox>
	</bottom>
</BorderPane>
